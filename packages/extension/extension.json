{
  "id": "builtin.prettier",
  "name": "Prettier",
  "description": "Code formatter using prettier",
  "browser": "src/prettierMain.js",
  "icon": "./icon.png",
  "activation": ["onFormatting:css", "onFormatting:json"],
  "languages": [
    {
      "id": "json",
      "fileNames": [".prettierrc"]
    },
    {
      "id": "ignore",
      "fileNames": [".prettierignore"]
    }
  ]
}
