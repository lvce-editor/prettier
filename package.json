    "e2e:headless": "cd packages/e2e && npm run e2e:headless"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@lvce-editor/package-extension": "^0.0.5",
    "@lvce-editor/server": "^0.16.13",
