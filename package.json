    "lerna": "^7.1.3",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6",
    "@lerna/legacy-package-management": "^7.1.3"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
